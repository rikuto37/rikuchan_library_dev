<% @document_title = "会員情報編集" %>

<div class="card">
    <%= render 'commons/errors', model: @user %>
    <%= form_with model: @user do |f| %>
        <%= render 'form', f: f %>
        <div class="card-footer">
            <%= f.submit '編集を確定', class:"btn btn-primary" %>
            <%= link_to 'キャンセル', @user, class:"btn btn-sm btn-outline-dark" %>
            <%= link_to '一覧へ戻る', users_path, class:"btn btn-link" %>
        </div>
    <% end %>
</div>