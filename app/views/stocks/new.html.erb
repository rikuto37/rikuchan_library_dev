<% @document_title = "新規在庫登録" %>

<div class="card">
    <%= render 'commons/errors', model: @stock %>

    <div class="card container">
        <%= form_with model: @stock do |f| %>
            <%= render 'form', f: f %>
            <div class="card-footer">
            <%= f.submit '登録', class:"btn btn-danger" %>
            <%= link_to '在庫一覧へ', stocks_path, class:"btn btn-link" %>
        <% end %>
    </div>
</div>