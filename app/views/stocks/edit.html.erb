<% @document_title = "在庫情報編集" %>

<div class="card container">
    <%= render 'commons/errors', model: @stock %>
    <%= form_with model: @stock do |f| %>
        <%= render 'form', f: f %>
        <div class="card-footer">
            <%= f.submit '編集を確定', class:"btn btn-primary" %>
            <%= link_to '在庫詳細へ戻る', @stock, class:"btn btn-sm btn-outline-success" %>
            <%= link_to '在庫一覧へ', stocks_path, class:"btn btn-sm btn-outline-secondary" %>
        </div>
    <% end %>
</div>
