<% @document_title = "新規目録登録" %>

<div class="card">
    <%= render 'commons/errors', model: @document %>
    <div class="card container">
        <%= form_with model: @document do |f| %>
            <%= render 'form', f: f %>
            <div class="card-footer">
                <%= f.submit '登録', class:"btn btn-danger" %>
                <%= link_to '目録一覧へ', documents_path, class:"btn btn-sm btn-outline-secondary" %>
            </div>
        <% end %>
    </div>
</div>