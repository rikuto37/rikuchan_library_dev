<% @document_title = "目録情報編集" %>
<div class="card">
    <%= render 'commons/errors', model: @document %>
    <%= form_with model: @document do |f| %>
        <%= render 'form', f: f %>
        <div class="card-footer">
            <%= f.submit '編集を確定', class:"btn btn-primary" %>
            <%= link_to 'キャンセル', @document, class:"btn btn-sm btn-outline-dark" %>
            <%= link_to '一覧へ戻る', documents_path, class:"btn btn-link" %>
        </div>
    <% end %>
</div>
